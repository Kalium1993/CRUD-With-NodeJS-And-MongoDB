<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>CRUD in NodeJS</title>
</head>
<body>

<h2>Users<h2>
    <table border="1">
        <thead>
            <tr>
                <td>First Name</td>
                <td>Last Name</td>
                <td>Action</td>
            </tr>
        </thead>
        <tbody>
        <% data.forEach(function(details) { %>
            <tr>
                <td><%= details.name %></td>
                <td><%= details.surname %></td>
                <td><a href="/edit/<%= details._id %>">Edit</a> -
                     <a href="/delete/<%= details._id %>">Delete</a></td>
            </tr>
            <% }) %>
        </tbody>
    </table>
        <button><a href="/">New User</a></button>
</body>
</html>